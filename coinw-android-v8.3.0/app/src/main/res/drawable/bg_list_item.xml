<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@color/color_f9f9fc" android:state_focused="true" />
    <item android:drawable="@color/color_f9f9fc" android:state_pressed="true" />
    <item android:drawable="@color/color_f9f9fc" android:state_checked="true" />
    <item android:drawable="@color/color_f9f9fc" android:state_checkable="true" />
    <!-- 这个是选中的时候item显示背景色 -->
    <item android:drawable="@color/color_f9f9fc" android:state_selected="true" />
    <!-- 未选中时的背景颜色 -->
    <item android:drawable="@color/white" />
</selector>