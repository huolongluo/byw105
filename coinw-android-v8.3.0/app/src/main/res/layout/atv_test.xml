<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="30dp"
    android:paddingTop="20dp"
    android:paddingRight="30dp"
    android:paddingBottom="20dp">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_http_log"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="HTTP LOG"
                android:textColor="@color/white"
                android:textSize="16sp"
                android:visibility="visible" />
            <TextView
                android:id="@+id/tv_ws_log"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:layout_marginTop="10dp"
                android:text="WS LOG"
                android:textColor="@color/white"
                android:textSize="16sp"
                android:visibility="visible" />
            <TextView
                android:id="@+id/tv_test"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:layout_marginTop="10dp"
                android:text="测试"
                android:textColor="@color/white"
                android:textSize="16sp"
                android:visibility="visible" />
            <TextView
                android:id="@+id/tv_change_ip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="切换为IP方式"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_svr_online"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="SVR-Online(appn)"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_svr_online_web"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="SVR-Online(coinw.to)"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_svr63"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="SVR-63"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_svr_178_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="SVR-178.5"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_contract"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="合约"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_face"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="阿里FACE验证"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_safe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="安全验证"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_hbt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="HBT"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_welfare_center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="福利中心"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_depth_v2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="合并深度2.0"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_share"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="分享图片"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_websocket_gsoms"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="WebSocket-GSOMS订阅"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_websocket_gsoms_next"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="WebSocket-GSOMS取数据"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_websocket_gsoms_end"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="WebSocket-GSOMS反订阅"
                android:textColor="@color/white"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/tv_websocket_xkline"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_quote_input"
                android:gravity="center"
                android:padding="10dp"
                android:text="K线页改版本-增加的WebSocket"
                android:textColor="@color/white"
                android:textSize="16sp" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>