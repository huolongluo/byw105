                                    android:id="@+id/copy_iv5"
                                    android:layout_width="18dp"
                                    android:layout_height="18dp"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginRight="17dp"
                                    android:src="@mipmap/copy" />
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/bankAdress_ll"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:gravity="center_vertical"
                                android:paddingLeft="20dp">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="@string/a17"
                                    android:textColor="@color/ffb0adc2"
                                    android:textSize="12sp" />
                                <TextView
                                    android:id="@+id/bankAdress_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:text="----"
                                    android:textColor="@color/ff333333"
                                    android:textSize="12sp" />
                                <ImageView
                                    android:id="@+id/copy_iv6"
                                    android:layout_width="18dp"
                                    android:layout_height="18dp"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginRight="17dp"
                                    android:src="@mipmap/copy" />
                            </LinearLayout>
                            <RelativeLayout
                                android:id="@+id/qrCode_rl"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="15dp"
                                android:visibility="gone">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/a18"
                                    android:textColor="@color/ffb0adc2"
                                    android:textSize="12sp" />
                                <ImageView
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_alignParentRight="true"
