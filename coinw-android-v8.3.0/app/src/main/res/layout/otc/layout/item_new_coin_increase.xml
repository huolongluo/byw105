<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_56">
    <TextView android:id="@+id/tvRank"
        style="@style/TextView_14_main2"
        android:layout_width="@dimen/dp_24"
        android:layout_marginStart="@dimen/margin_horizontal"
        android:gravity="center_horizontal"
        android:layout_centerVertical="true"
        tools:text="4"/>
    <ImageView android:id="@+id/ivRank"
        android:layout_width="@dimen/dp_24"
        android:layout_height="wrap_content"
        tools:src="@mipmap/ic_new_coin_rank1"
        android:layout_marginStart="@dimen/margin_horizontal"
        android:layout_centerVertical="true"
        android:visibility="gone"/>
    <TextView android:id="@+id/tvIncreaseName"
        style="@style/TextView_14_main"
        android:layout_width="@dimen/dp_60"
        android:layout_marginStart="@dimen/dp_52"
        tools:text="Babysasffgh"
        android:layout_centerVertical="true" />
    <TextView android:id="@+id/tvOpenPrice"
        style="@style/TextView_14_main2"
        android:layout_width="@dimen/dp_76"
        android:layout_marginStart="@dimen/dp_120"
        tools:text="123125412"
        android:layout_centerVertical="true" />
    <TextView android:id="@+id/tvHighestPrice"
        style="@style/TextView_14_main2"
        android:layout_width="@dimen/dp_76"
        android:layout_marginStart="@dimen/dp_200"
        tools:text="14251251.41"
        android:layout_centerVertical="true" />
    <TextView android:id="@+id/tvHighestIncrease"
        style="@style/TextView_14_white"
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_30"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="@dimen/margin_horizontal"
        android:background="@drawable/bg_cor5_up"
        android:gravity="center"
        tools:text="@string/highest_increase"
        android:layout_centerVertical="true" />
</RelativeLayout>
