<?xml version="1.0" encoding="utf-8"?>
<layout>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/base_bg_white"
            android:orientation="vertical">
            <include
                android:id="@+id/include"
                layout="@layout/base_title"></include>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:background="@color/base_col"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="80dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="80dp"
                        android:layout_marginBottom="40dp"
                        android:src="@mipmap/prepaid_icon" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:background="@color/base_col"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="34.5dp"
                        android:layout_height="34dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="17dp"
                        android:src="@mipmap/complain_icon" />
                    <TextView
                        android:id="@+id/shensu_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="16dp"
                        android:text="@string/f11"
                        android:textColor="@color/white"
                        android:textSize="16sp" />
                </LinearLayout>
            </RelativeLayout>
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/base_bg_white">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:background="@color/white"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_weight="1"
                            android:text="@string/f12"
                            android:textColor="@color/ff333333"
                            android:textSize="14sp" />
                        <TextView
                            android:id="@+id/orderId_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="#**"
                            android:textColor="@color/ff333333"
                            android:textSize="12sp" />
                        <ImageView
                            android:id="@+id/copy_iv1"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginLeft="9dp"
                            android:layout_marginRight="17dp"
                            android:src="@mipmap/copy" />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/fff0eef6"></View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical"
                        android:paddingTop="30dp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/f13"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/iv5"
                                android:layout_width="6.5dp"
                                android:layout_height="11.5dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="18dp" />
                            <TextView
                                android:id="@+id/userName_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="8dp"
                                android:layout_toLeftOf="@+id/iv5"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/ddje"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/amount_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="18dp"
                                android:text="** CNY"
                                android:textColor="@color/ff6f6699"
                                android:textSize="16sp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/danjia"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/price_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="18dp"
                                android:text="** CNY"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/number"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/num_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="18dp"
                                android:text="** CNYT"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/paymark"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/copy_iv2"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="18dp"
                                android:src="@mipmap/copy" />
                            <TextView
                                android:id="@+id/transReferNum_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:layout_toLeftOf="@+id/copy_iv2"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_marginBottom="30dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/ordertime"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/createTime_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="18dp"
                                android:text="--"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                        </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="40dp"
                        android:background="@color/white"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="46dp"
                            android:gravity="center_vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="1"
                                android:text="@string/f14"
                                android:textColor="@color/ff333333"
                                android:textSize="14sp" />
                            <ImageView
                                android:id="@+id/payType_iv"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="9dp"
                                android:src="@mipmap/bank_cark" />
                            <TextView
                                android:id="@+id/payType_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="6dp"
                                android:layout_marginRight="17dp"
                                android:text="--"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/fff0eef6"></View>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="30dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/f15"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/otherName_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/copy_iv3"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="17dp"
                                android:src="@mipmap/copy" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:id="@+id/accountName_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/f17"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/account_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/copy_iv4"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="17dp"
                                android:src="@mipmap/copy" />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/bankName_ll"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/f18"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/bankName_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/copy_iv5"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="17dp"
                                android:src="@mipmap/copy" />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/bankAdress_ll"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/f19"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/bankAdress_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/copy_iv6"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="17dp"
                                android:src="@mipmap/copy" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_marginBottom="40dp"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/f20"
                                android:textColor="@color/ffb0adc2"
                                android:textSize="12sp" />
                            <TextView
                                android:id="@+id/transReferNum1_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="----"
                                android:textColor="@color/ff333333"
                                android:textSize="12sp" />
                            <ImageView
                                android:id="@+id/copy_iv7"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="17dp"
                                android:src="@mipmap/copy" />
                        </LinearLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="40dp"></View>
                </LinearLayout>
            </ScrollView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_alignParentBottom="true"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="horizontal">
            <RelativeLayout
                android:id="@+id/contact_ll"
                android:layout_width="0dp"
                android:layout_height="43dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/img"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@drawable/contact" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:layout_toRightOf="@id/img"
                    android:text="@string/f21"
                    android:textColor="#747281"
                    android:textSize="13sp"></TextView>
                <TextView
                    android:id="@+id/dot"
                    android:layout_width="9dp"
                    android:layout_height="9dp"
                    android:layout_alignTop="@id/img"
                    android:layout_alignRight="@id/img"
                    android:layout_marginTop="-5dp"
                    android:background="@drawable/notify"
                    android:gravity="center"
                    android:text="-"
                    android:textColor="@color/white"
                    android:textSize="6sp"
                    android:visibility="gone" />
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/cancel_action"
                android:layout_width="0dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:background="#6F6699"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:text="@string/f22"
                    android:textColor="@color/FFFFFF"
                    android:textSize="13sp" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</layout>