<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:paddingStart="@dimen/margin_horizontal"
        android:paddingEnd="@dimen/margin_horizontal"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1">
            <com.legend.common.view.textview.DinproBoldTextView
                android:id="@+id/zhang_name_tv1"
                style="@style/TextView_16_main_bold"
                android:singleLine="true"
                android:text="--"/>
            <com.legend.common.view.textview.DinproMediumTextView
                android:id="@+id/zhang_name_tv1_1"
                style="@style/TextView_10_8085889B"
                android:layout_alignBottom="@+id/zhang_name_tv1"
                android:layout_marginBottom="2dp"
                android:layout_toRightOf="@+id/zhang_name_tv1"
                android:singleLine="true"
                android:text="/--" />
        </RelativeLayout>
        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingEnd="@dimen/dp_20"
            android:layout_weight="1.4">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <com.legend.common.view.textview.DinproBoldTextView
                    android:id="@+id/zhang_price_tv1"
                    style="@style/TextView_16_main_bold"
                    android:maxLines="1"
                    android:text="--"
                    android:layout_alignParentRight="true"/>
                <com.legend.common.view.textview.DinproMediumTextView
                    android:id="@+id/zhang_price_tv1_1"
                    style="@style/TextView_10_main2"
                    android:layout_below="@+id/zhang_price_tv1"
                    android:layout_alignRight="@+id/zhang_price_tv1"
                    android:layout_marginTop="@dimen/dp_3"
                    android:singleLine="true"
                    android:text="Â¥--" />
            </RelativeLayout>
        </FrameLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:layout_weight="1">
            <com.legend.common.view.textview.DinproMediumTextView
                android:id="@+id/zhang_rate_tv1"
                style="@style/TextView_14_white"
                android:layout_width="@dimen/dp_80"
                android:layout_height="@dimen/btn_height"
                android:layout_alignParentRight="true"
                android:background="@drawable/bg_cor5_up"
                android:gravity="center"
                android:maxLines="1"
                tools:text="-12.4%"/>
            <LinearLayout
                android:id="@+id/zhang_rate_tv22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="right"
                android:orientation="vertical">
                <com.legend.common.view.textview.DinproMediumTextView
                    android:id="@+id/zhang_rate_tv2"
                    style="@style/TextView_16_main"
                    android:gravity="center"
                    android:maxLines="1"
                    android:text="--%"/>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>