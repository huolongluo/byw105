<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <!--android:background="@drawable/1login_bg"-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="33.5dp"
            android:layout_marginTop="3dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/iv_history_title"
                android:layout_width="30dp"
                android:layout_height="15dp"
                android:layout_marginLeft="17dp"
                android:background="@drawable/shape_buy"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:text="买入"
                android:textColor="@color/white"
                android:textSize="10sp" />
            <TextView
                android:id="@+id/coinName_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:lines="1"
                android:text="--  --"
                android:textColor="#1B1B1B"
                android:textSize="14sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/cnyName_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lines="1"
                android:textColor="#1B1B1B"
                android:textSize="14sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/tv_history_updat_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7.5dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="2014-10-16 19:12:21"
                android:textColor="@color/ff8881a6"
                android:textSize="9sp" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center_vertical"
                android:src="@mipmap/delete_1"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_statu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:text=""
                android:textColor="@color/ff8881a6"
                android:textSize="11sp" />
            <TextView
                android:id="@+id/iv_history_cancle_order"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:background="@drawable/shape_cancel"
                android:paddingLeft="7dp"
                android:paddingTop="2dp"
                android:paddingRight="7dp"
                android:paddingBottom="2dp"
                android:text="@string/reback"
                android:textColor="#303030"
                android:textSize="11sp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/ffd7dcea" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:gravity="center_vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:padding="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:paddingLeft="10dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/tv_history_fprice_tip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:text="@string/d20"
                                android:textColor="#A5A2BE"
                                android:textSize="10sp" />
                            <TextView
                                android:id="@+id/tv_history_fprice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:singleLine="true"
                                android:text="0.00"
                                android:textColor="@color/color_1B1B1B"
                                android:textSize="12sp" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/tv_history_fsuccessprice_tip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:text="@string/d21"
                                android:textColor="#A5A2BE"
                                android:textSize="10sp" />
                            <TextView
                                android:id="@+id/tv_history_fsuccessprice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:singleLine="true"
                                android:text="0.00"
                                android:textColor="@color/color_1B1B1B"
                                android:textSize="12sp" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:paddingLeft="20dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/tv_history_famount_tip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:text="@string/d18"
                                android:textColor="#A5A2BE"
                                android:textSize="10sp" />
                            <TextView
                                android:id="@+id/tv_history_famount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:singleLine="true"
                                android:text="0.00"
                                android:textColor="@color/color_1B1B1B"
                                android:textSize="12sp" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_weight="0"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/btb_c_user"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:lines="1"
                                android:text="@string/d19"
                                android:textColor="#A5A2BE"
                                android:textSize="10sp" />
                            <TextView
                                android:id="@+id/tv_history_successamount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:singleLine="true"
                                android:text="0.00"
                                android:textColor="@color/color_1B1B1B"
                                android:textSize="12sp" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/fee_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:paddingLeft="20dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/tv_fee_tip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:text="@string/fee"
                                android:textColor="#A5A2BE"
                                android:textSize="10sp" />
                            <TextView
                                android:id="@+id/fee_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:singleLine="true"
                                android:text="0.00"
                                android:textColor="@color/color_1B1B1B"
                                android:textSize="12sp" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_weight="0"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/tv_all_money_tip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:lines="1"
                                android:text="@string/all_money"
                                android:textColor="#A5A2BE"
                                android:textSize="10sp" />
                            <TextView
                                android:id="@+id/allMoney"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:singleLine="true"
                                android:text="0.00"
                                android:textColor="@color/color_1B1B1B"
                                android:textSize="12sp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>